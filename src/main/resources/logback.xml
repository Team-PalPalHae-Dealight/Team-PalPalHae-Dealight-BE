<configuration>
    <appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
        <encoder>
            <pattern>%d{HH:mm:ss.SSS} [%thread] %-5level %F, method:%M, %L line : %m%n</pattern>
        </encoder>
    </appender>
    <root level="debug">
        <appender-ref ref="info"/>
    </root>
    <logger name="com.palpal.dealightbe" level="debug">
        <appender-ref ref="info"/>
    </logger>
</configuration>
